{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/shop-core/src",
  "monorepo": true,
  "root": "apps/shop-core",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/shop-core/tsconfig.app.json"
  },
  "projects": {
    "shop-core": {
      "type": "application",
      "root": "apps/shop-core",
      "entryFile": "main",
      "sourceRoot": "apps/shop-core/src",
      "compilerOptions": {
        "tsConfigPath": "apps/shop-core/tsconfig.app.json"
      }
    },
    "products": {
      "type": "application",
      "root": "apps/products",
      "entryFile": "main",
      "sourceRoot": "apps/products/src",
      "compilerOptions": {
        "tsConfigPath": "apps/products/tsconfig.app.json"
      }
    }
  }
}